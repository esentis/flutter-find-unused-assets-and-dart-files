{
  "name": "flutter-find-unused-assets-and-dart-files",
  "publisher": "esentis",
  "icon": "icon.png",
  "displayName": "Flutter: Find unused assets and Dart files",
  "description": "Find unused assets & files to clean up your project.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unused-assets.findUnusedAssets",
        "title": "Flutter: Find Unused Dart Files & Assets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esentis/flutter-find-unused-assets-and-dart-files"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
